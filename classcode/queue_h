#ifndef queue_h
#define queue_h
#include<stdio.h>
#include<stdlib.h>
#include<stdbool.h>

#define MaxSize 10
typedef int DataType;
typedef struct{
    DataType data[MaxSize];
    int front;
    int rear;
}SeqQueue;

SeqQueue *initqueue();
bool emptyQueue(SeqQueue *queue);
bool fullQueue(SeqQueue *queue);
void inQueue(q->rear=(q->rear+1)%MAXSIZE,DataType data);
void deQueue(q->front=(q->front+1)%MAXSIZE,DataType *data);
void frontQueue(SeqQueue *queue,DataType *data);
void rearQueue(SeqQueue *queue,DataTpye *data);

#endif     
