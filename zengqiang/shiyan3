#include<stdio.h>
#include<stdlib.h>
#include<time.h>
#include<sys/timeb.h>
#define MAXSIZE 20
#define MAXNUM 21
void Swap(int* a,int* b)
{
     int temp = *a;
          *a  = *b;
          *b  = temp;
}
void InsertSort_Up(int* arr,int length)
{
    for (int i= 1;i<length;i++)
    {
         int temp = arr[i];
         int j;
         for (j =i-1;j>=0 && arr[j] > temp ;j--)
         {
             arr[j+1]=arr[j];
         }
   arr[j +1] =temp;
}
return;
}
void InsertSort_Down(int* arr,int length)
{
    for(int i=1;i<length;i++)
 {
    int temp =arr[i];
    int j;
    for (j =i-1;j>=0 && arr[j]< temp;j--)
    {
       arr[j+1] =arr[j];
       }
       arr[j+1]=temp;
 }
 return;
void PrintArr(int* arr,int length)
{
   for(int i=0;i<length;i++)
{
   printf("\n");
   return;
}
void PrintArr (int* arr,int length)
{
    for(int i=0;i<length;i++)
    {
      printf("%d",arr[i];
    }
 printf("\n");
 return;
}
int main(int argc,char *argv[])
{
   srand(size_t)time(NULL));
   int arr[MAXSIZE] ={0};
   for (int i=0;i<MAXSIZE;i++)
  {
    int num=rand() %MAXSIZE;
    arr[i] = num;
  }
  printf("排序前:\n");
  PrintArr(arr ,MAXSIZE);
  printf("直接插入排序升序：\n");
  InsertSort_Up(arr,MAXSIZE)
  PrintArr(arr,MAXSIZE);
  printf("直接插入排序降序：\n");
  InsertSort_Down(arr ,MAXSIZE);
  PrintArr(arr ,MAXSIZE);
  return o;
  }
   
