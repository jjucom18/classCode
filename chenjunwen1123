      #include<stdio.h>
      int main(int argc, char **agrv){ 
      int varl, var2, var3, temp;
      int *ptr_varl=&varl,*ptr_var2=&var2,*ptr_var3=&var3;
      scanf("%d, %d, %d" , &varl, &var2, &var3);
      printf ("var1=%d, var2=%d, var3=%d\n",varl,var2,var3);
      printf("%p, %p, %p\n", &varl, &var2, &var3);
      temp=*ptr_varl;
      *ptr_varl=*ptr_var3;
      *ptr_var3=*ptr_var2;
      *ptr_var2=temp;
      printf ("var1=%d,var2=%d,var3=%d\n",varl,var2,var3);
      }
